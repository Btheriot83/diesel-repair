{
  "name": "astro-fresh",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run optimize-images && astro build",
    "build:fast": "astro build",
    "build:analyze": "ANALYZE=true astro build",
    "build:production": "NODE_ENV=production npm run optimize-images && astro build",
    "preview": "astro preview",
    "preview:build": "npm run build && astro preview",
    "astro": "astro",
    "optimize:images": "node scripts/optimize-images.mjs",
    "optimize:images:critical": "node scripts/optimize-images.mjs --critical",
    "optimize:images:dry-run": "node scripts/optimize-images.mjs --dry-run",
    "optimize:svgs": "node scripts/optimize-svgs.mjs",
    "lighthouse": "lhci autorun",
    "performance-check": "npm run build && npm run lighthouse",
    "verify:home": "node scripts/verify-homepage-seo.mjs",
    "verify:hero": "node tools/hero-guard.mjs",
    "verify:sab": "node tools/sab-lint.mjs",
    "verify:meta": "node tools/meta-check.mjs",
    "verify:links": "node tools/links-check.mjs",
    "verify:images": "node tools/image-budget.mjs",
    "verify:hygiene": "node tools/site-hygiene.mjs",
    "verify:phone": "node tools/phone-usage-check.mjs",
    "verify:locations": "npm run verify:hero && node tools/uniqueness-check.mjs && npm run verify:sab && npm run verify:meta && npm run verify:links && npm run verify:images && npm run verify:hygiene && npm run verify:phone"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.3.1",
    "@astrojs/tailwind": "^6.0.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.9",
    "js-yaml": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2"
  },
  "description": "```sh npm create astro@latest -- --template minimal ```",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Btheriot83/diesel-repair.git"
